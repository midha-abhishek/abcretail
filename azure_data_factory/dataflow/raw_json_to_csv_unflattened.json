{
	"name": "raw_json_to_csv_unflattened",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_dlsa_abcretail_http_raw_json",
						"type": "DatasetReference"
					},
					"name": "rawJSON"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_dlsa_abcretail_http_raw_csv",
						"type": "DatasetReference"
					},
					"name": "rawCSV"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'arrayOfDocuments') ~> rawJSON",
				"rawJSON sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> rawCSV"
			]
		}
	}
}